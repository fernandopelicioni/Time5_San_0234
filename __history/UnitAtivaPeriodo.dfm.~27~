  object QrPeriodos: TADOQuery
    AutoCalcFields = False
    Connection = Pai.Genoma
    CursorType = ctStatic
    Parameters = <
      item
        Name = 'USUARIO'
        Size = -1
        Value = Null
      end
      item
        Name = 'COLIGADA'
        Size = -1
        Value = Null
      end>
    Prepared = True
    SQL.Strings = (
      ''
      'Select '
      '                    TPE_COD, '
      #9#9'            TPE_DES, '
      #9#9#9#9#9'Convert(varchar(10),TPE_INICIO,103) as TPE_INICIO, '
      #9#9#9#9#9'Convert(varchar(10),TPE_FIM,103) as TPE_FIM, '
      #9#9#9#9#9'Convert(varchar(10),TPE_2SEM,103) AS TPE_2SEM,'
      #9#9#9#9#9'Convert(varchar(10),TPE_3SEM,103) as TPE_3SEM,'
      #9#9#9#9#9'Convert(varchar(10),TPE_4SEM,103) as TPE_4SEM,'
      #9#9#9#9#9'Convert(varchar(10),TPE_5SEM,103) as TPE_5SEM,'
      #9#9#9#9#9'tPE_fOLGAS,'
      'PTUSU_ID,'
      #9#9#9#9#9'USU_LOGIN,'
      #9#9#9#9#9'PTUSU_PERIODO'
      'from '
      '           Tb_PtoPeriodos A'
      'left  Join '
      '           TB_PTOUSUARIOS B On PTUSU_COLIGADA = A.TPE_COLIGADA '
      #9#9'   and B.PTUSU_PERIODO = A.TPE_COD AND PTUSU_USUARIO =:usuario'
      #9#9'   '
      'LEFT JOIN '
      '           TB_USUARIOS C ON C.USU_CODIGO = B.PTUSU_USUARIO'
      'WHERE '
      '           TPE_COLIGADA =:coligada'
      ''
      ''
      'ORDER BY TPE_ANO, TPE_MES DESC')
    Left = 280
    Top = 288
  end
